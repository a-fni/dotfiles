[AC][Display]
DimDisplayIdleTimeoutSec=-1
DimDisplayWhenIdle=false
DisplayBrightness=100
TurnOffDisplayIdleTimeoutSec=1800
UseProfileSpecificDisplayBrightness=true

[Battery][Display]
DimDisplayIdleTimeoutSec=-1
DimDisplayWhenIdle=false
